@page "/"
@using Microsoft.AspNetCore.Components.Web
@namespace Spillgebees.Blazor.Map.Samples.Server.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<!DOCTYPE html>
<html class="dark" lang="en">
<head>
    <meta charset="utf-8" />
    <title>Spillgebees.Blazor.Map.Samples.Server</title>
    <base href="~/" />
    <link href="favicon.ico" rel="icon" type="image/x-icon" />
    <component type="typeof(HeadOutlet)" render-mode="ServerPrerendered" />

    <link href="css/site.css" rel="stylesheet" />

    <!-- for presentation purposes only -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Roboto+Mono&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@@catppuccin/highlightjs@1.0.1/css/catppuccin-mocha.css">
    <script src="https://cdn.jsdelivr.net/npm/@@tailwindcss/browser@4"></script>
</head>
<body>
    <component type="typeof(App)" render-mode="ServerPrerendered" />

    <div id="blazor-error-ui">
        <environment include="Staging,Production">
            An error has occurred. This application may no longer respond until reloaded.
        </environment>
        <environment include="Development">
            An unhandled exception has occurred. See browser dev tools for details.
        </environment>
        <a href="" class="reload">Reload</a>
        <a class="dismiss">ðŸ—™</a>
    </div>

    <script src="_framework/blazor.server.js"></script>

    <!-- for presentation purposes only -->
    <script type="text/javascript"
        src="https://cdn.jsdelivr.net/gh/highlightjs/cdn-release@11.11.1/build/highlight.min.js"></script>
    <script type="text/javascript"
        src="https://cdn.jsdelivr.net/npm/highlightjs-cshtml-razor@2.2.0/dist/cshtml-razor.min.js"></script>
</body>
</html>
