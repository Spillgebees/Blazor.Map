  "packageManager": "pnpm@10.30.2",
  "description": "TypeScript module housing assets related to the Spillgebees.Blazor.Map NuGet package.",
  "author": "Jason Rebelo Neves (igotinfected)",
  "license": "MIT",
  "repository": {
